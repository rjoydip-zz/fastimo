{
  "name": "fastlib",
  "version": "0.0.1",
  "description": "Faster library for scalable architecture",
  "main": "index.js",
  "devDependencies": {
    "eslint": "^3.1.1",
    "jest": "^23.5.0",
    "nodemon": "^1.9.2"
  },
  "scripts": {
    "start": "./node_modules/.bin/nodemon index.js",
    "lint": "eslint . --ignore-path .gitignore --cache",
    "fix:lint": "npm run lint -- --fix",
    "precommit": "npm run lint",
    "test": "./node_modules/.bin/jest --coverage",
    "prepush": "npm run lint && npm run test"
  },
  "keywords": [
    "faster",
    "library",
    "scalable"
  ],
  "author": {
    "name": "Joydip Roy",
    "email": "joydipand@gmail.com",
    "url": "http://github.com/rjoydip"
  },
  "license": "MIT",
  "dependencies": {
    "husky": "^1.0.0-rc.13"
  },
  "readme": "README.md",
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run prepush"
    }
  }
}
